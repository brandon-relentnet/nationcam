# PostgreSQL
POSTGRES_DB=nationcam
POSTGRES_USER=nationcam
POSTGRES_PASSWORD=change_me_in_production

# PostgREST JWT secret (min 32 characters)
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
PGRST_JWT_SECRET=change_me_min_32_chars_long_secret_here

# App build-time variables
VITE_PAGE_PASSWORD=your_admin_page_password
VITE_ADMIN_JWT=

# Host port for the app (default 3000)
APP_PORT=3000
